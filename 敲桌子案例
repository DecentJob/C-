#include <iostream>
#include <string>
#include <ctime>
using namespace std;

int main()
{
	//敲桌子案例
	//1.输出1-100的数字；
	int i = 1;
	for (i = 1; i <= 100; i++)
	{
		//2.从1-100中找到特殊数字；
		if (i%7==0||i%10==7||i/10==7)
		{

			cout << "敲桌子" << endl;
		}
		else
		{
			cout << i<< endl;
		}


	}
	system("pause");


	return 0;
}
